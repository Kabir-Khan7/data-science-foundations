data = np.array([[10,20,30],
                 [15,25,35],
                 [20,30,40],
                 [25,35,45],
                 [30,40,50]])

mean = data.mean(axis=0)
std = data.std(axis=0)

print(f"Data: {data}")
print(f"Mean of the data: {mean}")
print(f"Std Deviation of the data: {std}")

normalized_data = [data - mean] / std
print(f"Normalized Data: {normalized_data}")




