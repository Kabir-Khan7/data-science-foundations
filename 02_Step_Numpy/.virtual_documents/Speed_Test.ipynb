


import numpy as np 
import time


# Using Python List
size = 1_000_0000

l1 = list(range(size))
l2 = list(range(size))

start = time.time()

add = [x+y for x,y in zip(l1, l2)]
end = time.time()
print(add[0:10])

print(end - start)


# Using Numpy Array
size = 1_000_000_

l1 = np.array(list(range(size)))
l2 = np.array(list(range(size)))

start = time.time()

add = l1 + l2
print(add[0:10])
end = time.time()

print(end - start)


