import pandas as pd


pd.__version__


df = pd.read_excel("fin_data.xlsx")
df


df.info()


df.describe()


df.isnull().sum()


for col in ['Close', 'P/E Ratio', 'Dividend Yield', 'Dividend']:
    df[col] = pd.to_numeric(df[col], errors='coerce')

df['Dividend'] = df['Dividend'].fillna(0).abs()
df['Dividend Yield'] = df['Dividend Yield'].abs()

df[['High', 'Low', 'Close', 'Volume']] = df.groupby('Ticker')[['High', 'Low', 'Close', 'Volume']].ffill() #Fill missing High/Low/Close/Volume within each stock by carrying forward the last known price.
df['P/E Ratio'] = df.groupby('Ticker')['P/E Ratio'].transform(lambda x: x.fillna(x.mean())) #Fill missing P/E values for a stock using the stock’s mean P/E.
df['Dividend Yield'] = df.groupby('Ticker')['Dividend Yield'].transform(lambda x: x.fillna(x.mean())) #Fill missing Dividend Yield values for a stock using the stock’s mean Dividend Yield.

print(df.isnull().sum())


df











